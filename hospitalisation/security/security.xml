<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hospitalisation_category" model="ir.module.category">
        <field name="name">Hospitalisation</field>
        <field name="sequence" eval="10" />
        <field name="description">Category For hospitalisation</field>
    </record>

    
    <record id="group_hospital_manager" model="res.groups">
        <field name="name">Manager</field>
        <!-- <field name="comment">Comment</field> -->
        <field name="category_id" ref="hospitalisation_category"/>
        <!-- <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(5, ref('base.user_root'))]" /> -->
    </record>

    <record id="group_hospital_visualisation" model="res.groups">
        <field name="name">Visualiseur</field>
        <!-- <field name="comment">Comment</field> -->
        <field name="category_id" ref="hospitalisation_category"/>
        <!-- <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(5, ref('base.user_root'))]" /> -->
    </record>
    
    <record id="group_cardio_b" model="res.groups">
        <field name="name">Cardio B</field>
        <field name="category_id" ref="base.module_category_hidden"/>
    </record>
</odoo>

